<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Torneos: Torneos de "La Banda del Lobo"</title>
    <link
      rel="shortcut icon"
      href="https://i.imgur.com/j6itbSk.png"
      type="image/x-icon"
    />
    <link href="css/style.css" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div class="team-grid">
      <% tournaments.map((tournamentEl)=>{ %>
      <div class="team-item">
        <button
          type="button"
          onclick= "location.href='/torneos/<%= tournamentEl.name %>'"
        >
          <img
            width="150rem"
            height="150rem"
            src="<%= tournamentEl.logo %>"
            alt="<%= tournamentEl.name %>"
          />
        </button>
      </div>
      <% }) %>
    </div>
  </body>
</html>
